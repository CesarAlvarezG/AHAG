#include "MIO.h"

//////////////////////////////VARIABLES GLOBALES////////////////////////////////


//////////////////////////////FUNCIONES/////////////////////////////////////////

void Mi_strcpy(char *original, char *copia)
{
 char a[5]={"NULL"};
 if(copia==NULL)
   {
    strcpy(original,a);
   }
 else strcpy(original,copia);
}

char Mi_strcmp(char *original, char *copia)
{
 if(original==NULL)
   {
    return 1;
   }
 if(copia==NULL)
   {
    return 1;
   }
 return strcmp(original,copia);
}

